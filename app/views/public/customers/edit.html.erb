<h1>会員情報編集</h1>
<span>情報登録</span>
<%= form_with model:@customers, local:true do |f| %>
  <!--<span><%= f.text_field :last_name %></span>-->
  <!--<%= f.text_field :first_name %>-->
<table>
  <tr>
    <th>氏名</th><th><%= f.text_field :last_name %><%= f.text_field :first_name %></th>
  </tr>
  <tr>
    <th>カナ</th><th><%= f.text_field :last_name_kana %><%= f.text_field :first_name_kana %></th>
  </tr>
  <tr>
    <th>郵便番号</th><th><%= f.text_field :postal_code %></th>
  </tr>
  <tr>
    <th>住所</th><th><%= f.text_field :address %></th>
  </tr>
  <tr>
    <th>電話番号</th><th><%= f.text_field :telephone_number %></th>
  </tr>
  <tr>
    <th>メールアドレス</th><th><%= f.text_field :email %></th>
  </tr>
</table>
<span><%= f.submit '編集内容を保存' %></span>
<span><%= link_to "退会する", withdrawal_path %></span>
<% end %>