<h1>配送先編集</h1>
<%= form_with model:@address, url:delivery_path,local:true do |f| %>
<table>
  <tr>
    <th>郵便番号</th>
    <th><%= f.text_field :postal_code %></th>
  </tr>
  <tr>
    <th>住所</th>
    <th><%= f.text_field :addresses %></th>
  </tr>
  <tr>
    <th>宛名</th>
    <th><%= f.text_field :name %></th>
  </tr>
</table>
<%= f.submit "変更内容を保存" %>
<% end %>